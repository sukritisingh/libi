<?php

/**
 * @file
 * Functions to support theming in the umd theme.
 */

use Drupal\Component\Utility\Html;

/**
 * Implements template_preprocess_message().
 */
function umd_preprocess_message(&$variables) {
}

function umd_preprocess_block(&$variables) {
    $node = \Drupal::request()->attributes->get('node');
    if($node){
      $variables['node_title'] = $node->getTitle();
    }    
}